{
    "name": "hnhdigital-os/laravel-frontend-asset-loader",
    "description": "Provides a simplified approach to loading assets",
    "keywords": ["laravel","illuminate","assets"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@hnh.digital"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "phpmd/phpmd" : "~2.0",
        "squizlabs/php_codesniffer" : "~3.0"
    },
    "suggest": {
        "hnhdigital-os/laravel-frontend-asset-pack": "H&H Digital asset pack"
    },
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "HnhDigital\\LaravelFrontendAssetLoader\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "process-timeout": 2000,
        "sort-packages": true,
        "platform": {
            "php": "7.3.0"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HnhDigital\\LaravelFrontendAssetLoader\\ServiceProvider"
            ]
        }
    }
}
