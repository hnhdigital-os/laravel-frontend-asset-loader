{
    "name": "hnhdigital-os/laravel-frontend-asset-loader",
    "description": "Provides a simplified approach to loading assets",
    "keywords": ["laravel","illuminate","assets"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@hnh.digital"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/hnhdigital-os/laravel-assets.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "roumen/asset": "v2.5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "illuminate/database": "4.*|5.*",
        "symfony/process": "~2.3",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "suggest": {
        "hnhdigital-os/laravel-frontend-assets": "H&H|Digital standard assets"
    },
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "HnhDigital\\LaravelFrontendAssetLoader\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HnhDigital\\LaravelFrontendAssetLoader\\ServiceProvider"
            ]
        }
    }
}
